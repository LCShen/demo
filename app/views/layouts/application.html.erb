<!DOCTYPE html>
<html>
<head>
  <title>Demo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<!-- New header -->
<header class="navbar navbar-fix-top navbar-inverse">
	<div class="navbar-inner">
		<div class="container">
			<%= link_to "Demo", "#", id:"logo" %>
		</div>
	</div>
</header>

<!-- Content wrapped in container -->
<div class="container">
	<%= yield %>
	<!-- New footer -->
	<small>
		<a>This is a test</a>
	</small>
</footer>

<!-- Debug footer -->
<%= debug(params) if Rails.env.development? %>
</div>

</body>
</html>
